<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>

<html>
<head>
<link href="<%=request.getContextPath()%>/res/css/signature-pad.css"
	rel="stylesheet" />


<script type="text/javascript">

	var _gaq = _gaq || [];
	_gaq.push([ '_setAccount', 'UA-39365077-1' ]);
	_gaq.push([ '_trackPageview' ]);

	(function() {
		var ga = document.createElement('script');
		ga.type = 'text/javascript';
		ga.async = true;
		ga.src = ('https:' == document.location.protocol ? 'https://ssl'
				: 'http://www')
				+ '.google-analytics.com/ga.js';
		var s = document.getElementsByTagName('script')[0];
		s.parentNode.insertBefore(ga, s);
	})();
</script>
</head>
<body onselectstart="return false">

	<div id="signature-pad" class="m-signature-pad">
		<div class="m-signature-pad--body">
			<canvas></canvas>
		</div>
		<div class="m-signature-pad--footer">
			<div class="description">请在上面的方框内签字</div>
			<div class="left">
				<button type="button" class="button clear" data-action="clear">清除</button>
			</div>
			<div class="right">
				<button type="button" class="button save" data-action="save-png">完成</button>
				 <button type="button" style="display: none" class="button save" data-action="save-svg">生成SVG</button> 
			</div>
		</div>
	</div>
	<div>

		<img
			src="data:image/png;base64," />
	</div>
<script src="<%=request.getContextPath()%>/res/js/signature_pad.js"></script>
<script src="<%=request.getContextPath()%>/res/js/app.js"></script>
</body>
</html>
